{
  "manifest_version": 2,
  "name": "World's Most Dangerous Chrome Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["history.js"]
    }
  ],
  "background": {
    "scripts": ["logCurrentSite.js", "redirect.js"]
  },
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
